new file mode 100644
index 0000000..7181cb4
--- /dev/null
+++ b/src/java/com/sun/image/codec/jpeg/JPEGCodec.java
@@ -0,0 +1,19 @@
+package com.sun.image.codec.jpeg;
+
+import java.awt.image.Raster;
+import java.io.OutputStream;
+
+public class JPEGCodec {
+
+    private static String msg = "Proprietary com.sun.image.codec.jpeg package not supported";
+
+    public static JPEGImageEncoder createJPEGEncoder(OutputStream output,
+            JPEGEncodeParam jpegEncodeParam) {
+        throw new UnsupportedOperationException(msg);
+    }
+
+    public static JPEGEncodeParam getDefaultJPEGEncodeParam(Raster raster, int jpegColorID) {
+        throw new UnsupportedOperationException(msg);
+    }
+
+}
diff --git a/src/java/com/sun/image/codec/jpeg/JPEGDecodeParam.java b/src/java/com/sun/image/codec/jpeg/JPEGDecodeParam.java
new file mode 100644
